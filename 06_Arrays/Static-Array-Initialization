import java.util.Scanner;

public class Arrayyy {
    public static void main(String[] args) {
        
        //declare array
        int arr[] =  {1,2,3,4,5};
        

        //print the array
        System.out.println("The array elemnts are :");
        for(int i=0;i<arr.length;i++){
            System.out.print(arr[i]+ " ");
        }


    }
    
}
